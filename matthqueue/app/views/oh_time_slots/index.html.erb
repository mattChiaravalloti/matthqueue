<p id="notice"><%= notice %></p>

<h1>Oh Time Slots</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oh_time_slots.each do |oh_time_slot| %>
      <tr>
        <td><%= link_to 'Show', oh_time_slot %></td>
        <td><%= link_to 'Edit', edit_oh_time_slot_path(oh_time_slot) %></td>
        <td><%= link_to 'Destroy', oh_time_slot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

